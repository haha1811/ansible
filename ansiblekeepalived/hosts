[lbservers]
lb1 ansible_host=10.100.2.11 hostname=lb1 ansible_user=ubuntu
lb2 ansible_host=10.100.2.12 hostname=lb2 ansible_user=ubuntu


[lbservers:vars]
ansible_ssh_private_key_file=/root/05004_lab_test_haha.pem
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'


[apservers]
ap1 ansible_host=10.100.2.21 hostname=ap1 ansible_user=ubuntu
ap2 ansible_host=10.100.2.22 hostname=ap2 ansible_user=ubuntu

[apservers:vars]
ansible_ssh_private_key_file=/root/05004_lab_test_haha.pem
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'
